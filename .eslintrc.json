{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "comma-dangle": 0,
    "prettier/prettier": ["error"]
  }
}
